{
    "version": 6,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 23,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "base",
            "hidden": true,
            "generator": "Ninja Multi-Config",
            "binaryDir": "${sourceDir}/build/${presetName}",
            "warnings": {
                "dev": true,
                "deprecated": true,
                "uninitialized": true,
                "unusedCli": true,
                "systemVars": false
            },
            "errors": {
                "dev": true,
                "deprecated": true
            },
            "cacheVariables": {
                "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
                "CMAKE_CONFIGURATION_TYPES": "Debug;Release;RelWithDebInfo"
            }
        },
        {
            "name": "gcc-17",
            "inherits": "base",
            "displayName": "GCC 17",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "gcc-17",
                "CMAKE_CXX_COMPILER": "g++-17"
            }
        },
        {
            "name": "gcc-23",
            "inherits": "base",
            "displayName": "GCC 23",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "gcc-23",
                "CMAKE_CXX_COMPILER": "g++-23"
            }
        },
        {
            "name": "gcc-latest",
            "inherits": "base",
            "displayName": "GCC Latest",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "gcc",
                "CMAKE_CXX_COMPILER": "g++"
            }
        },
        {
            "name": "clang-17",
            "inherits": "base",
            "displayName": "Clang 17",
